{
    "name": "sunnysideup/move-large-files-to-assets",
    "description": "Move (large) files from git repo to assets in case uploading is difficult",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "Silverstripe-CMS",
        "CMS",
        "Silverstripe",
        "upload",
        "assets",
        "hacks",
        "files",
        "large files"
    ],
    "require": {
        "silverstripe/cms": "4.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nicolaas Francken",
            "email": "modules@sunnysideup.co.nz",
            "role": "lead developer"
        }
    ],
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "Sunnysideup\\MoveLargeFilesToAssets\\": "src/"
        }
    }
}